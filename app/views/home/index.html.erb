<h1>EWL SMRT Disruption</h1>

<table>
  <% (Date.new(2017, 01, 01)..Date.new(2017, 12, 31)).each_with_index do |date, index| %>
    <% if (index+1)%31 == 1 %>
    <tr>
    <% end %>
      <% if @timestampArray.include? date %>
      <td><div class="false">1</div></td>
      <% else %>
      <td><div class="true">0</div></td>
      <% end %>
      <td><%= date %></td>
    <% if (index+1)%31 == 0 %>
    </tr>
    <% end %>
  <% end %>
</table>
